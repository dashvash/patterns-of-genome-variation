{
    "input": "..\/data\/fastq\/VK*.fastq",
    "single_end": true,
    "large_ref": true,
    "fasta": "..\/data\/ref\/Homo_sapiens_assembly19.fasta", 
    "skip_fastqc": true,
    "skip_adapterremoval": true,
    "skip_preseq": true,
    "skip_deduplication": true,
    "skip_damage_calculation": true,
    "skip_qualimap": true,
    "clip_forward_adaptor": "CTGTCTCTTATACACATCT",
    "clip_reverse_adaptor": "ATGTGTATAAGAGACA",
    "bwaalnl": "",
    "run_bam_filtering": true,
    "bam_mapping_quality_threshold": 30,
    "dedupper": "dedup",
    "damageprofiler_length": 300,
    "run_genotyping": true,
    "genotyping_tool": "ug",
    "run_multivcfanalyzer": true,
    "write_allele_frequencies": true,
    "min_base_coverage": 3
}
